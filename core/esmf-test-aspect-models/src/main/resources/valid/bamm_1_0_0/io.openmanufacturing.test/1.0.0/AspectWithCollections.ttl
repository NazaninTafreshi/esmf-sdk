@prefix : <urn:samm:org.eclipse.esmf.samm.test:1.0.0#> .
@prefix bamm: <urn:samm:org.eclipse.esmf.samm:meta-model:1.0.0#> .
@prefix bamm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:1.0.0#> .
@prefix bamm-e: <urn:samm:org.eclipse.esmf.samm:entity:1.0.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithCollections a bamm:Aspect ;
   bamm:name "AspectWithCollections" ;
   bamm:properties ( :setProperty :listProperty ) ;
   bamm:operations ( ) .

:listProperty a bamm:Property ;
   bamm:name "listProperty" ;
   bamm:characteristic [
      a bamm-c:List ;
      bamm:name "NumberList" ;
      bamm:dataType xsd:int
   ] .

:setProperty a bamm:Property ;
   bamm:name "setProperty" ;
   bamm:characteristic [
      a bamm-c:Set ;
      bamm:name "StringSet" ;
      bamm:dataType xsd:string
   ] .



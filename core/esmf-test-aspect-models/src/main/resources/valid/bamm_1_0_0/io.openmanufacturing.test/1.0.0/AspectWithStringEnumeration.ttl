@prefix : <urn:samm:org.eclipse.esmf.samm.test:1.0.0#> .
@prefix bamm: <urn:samm:org.eclipse.esmf.samm:meta-model:1.0.0#> .
@prefix bamm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:1.0.0#> .
@prefix bamm-e: <urn:samm:org.eclipse.esmf.samm:entity:1.0.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithStringEnumeration a bamm:Aspect ;
   bamm:name "AspectWithStringEnumeration" ;
   bamm:properties ( :enumerationProperty ) ;
   bamm:operations ( ) .

:enumerationProperty a bamm:Property ;
   bamm:name "enumerationProperty" ;
   bamm:preferredName ""@en ;
   bamm:description ""@en ;
   bamm:characteristic [
     a bamm-c:Enumeration ;
     bamm:name "Foo" ;
     bamm:dataType xsd:string ;
     bamm-c:values ( "foo" "bar" )
   ] .


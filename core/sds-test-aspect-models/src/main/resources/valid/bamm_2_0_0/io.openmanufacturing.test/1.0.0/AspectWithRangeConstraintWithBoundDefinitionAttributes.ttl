@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithRangeConstraintWithBoundDefinitionAttributes a bamm:Aspect ;
   bamm:name "AspectWithRangeConstraintWithBoundDefinitionAttributes" ;
   bamm:properties ( :testProperty ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:name "testProperty" ;
   bamm:characteristic :TestTrait .

:TestTrait a bamm-c:Trait ;
   bamm:name "TestTrait" ;
   bamm-c:constraint [
      a bamm-c:RangeConstraint ;
      bamm:name "TestConstraint" ;
      bamm:preferredName "Test Constraint"@en ;
      bamm:description "Test Constraint"@en ;
      bamm:see <http://bosch.com/opcon> ;
      bamm-c:minValue "5"^^xsd:nonNegativeInteger ;
      bamm-c:lowerBoundDefinition bamm-c:GREATER_THAN ;
      bamm-c:maxValue "10"^^xsd:nonNegativeInteger ;
      bamm-c:upperBoundDefinition bamm-c:LESS_THAN ;
   ] ;
   bamm-c:baseCharacteristic [
      a bamm:Characteristic ;
      bamm:name "NumberCharacteristic" ;
      bamm:dataType xsd:nonNegativeInteger
   ] .

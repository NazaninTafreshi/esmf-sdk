@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithUsedAndUnusedConstraint a bamm:Aspect ;
   bamm:name "AspectWithUsedAndUnusedConstraint" ;
   bamm:properties ( :testProperty ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:name "testProperty" ;
   bamm:characteristic :UsedTestTrait .

:UsedTestTrait a bamm-c:Trait ;
   bamm:name "UsedTestTrait" ;
   bamm-c:constraint [
      a bamm-c:LengthConstraint ;
      bamm:name "UsedTestConstraint" ;
      bamm:preferredName "Used Test Constraint"@en ;
      bamm:description "Used Test Constraint"@en ;
      bamm:see <http://bosch.com/opcon> ;
      bamm:see <http://bosch.com/me> ;
      bamm-c:minValue "5"^^xsd:nonNegativeInteger ;
      bamm-c:maxValue "10"^^xsd:nonNegativeInteger ;
   ] ;
   bamm-c:baseCharacteristic bamm-c:Text .

:UnusedTestConstraint a bamm-c:LengthConstraint ;
   bamm:name "UnusedTestConstraint" ;
   bamm:preferredName "Unused Test Constraint"@en ;
   bamm:description "Unused Test Constraint"@en ;
   bamm:see <http://bosch.com/opcon> ;
   bamm:see <http://bosch.com/me> ;
   bamm-c:minValue "5"^^xsd:nonNegativeInteger ;
   bamm-c:maxValue "10"^^xsd:nonNegativeInteger .

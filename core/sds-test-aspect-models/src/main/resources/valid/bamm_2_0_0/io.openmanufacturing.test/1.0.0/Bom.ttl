@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:2.0.0#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:2.0.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .

:Bom a bamm:Aspect ;
   bamm:name "Bom" ;
   bamm:properties ( :bomItem :bomItems ) ;
   bamm:operations ( ) .

:bomItem a bamm:Property ;
   bamm:name "bomItem" ;
   bamm:characteristic :BomItemCharacteristic .

:bomItems a bamm:Property ;
   bamm:name "bomItems" ;
   bamm:characteristic :BomItemsCollection .

:BomItemCharacteristic a bamm-c:SingleEntity ;
   bamm:name "BomItemCharacteristic" ;
   bamm:dataType :BomItem .

:BomItemsCollection a bamm-c:Collection ;
   bamm:name "BomItemsCollection" ;
   bamm:dataType :BomItem .

:BomItem a bamm:Entity ;
   bamm:name "BomItem" ;
   bamm:properties ( :content [ bamm:property :parent; bamm:optional true ] ) .

:content a bamm:Property ;
   bamm:name "content" ;
   bamm:characteristic bamm-c:Text .

:parent a bamm:Property ;
   bamm:name "parent" ;
   bamm:characteristic :BomItemCharacteristic .


@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:2.0.0#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithOptionalProperties a bamm:Aspect ;
   bamm:name "AspectWithOptionalProperties" ;
   bamm:properties ( [ bamm:property :numberProperty; bamm:optional true ] :timestampProperty ) ;
   bamm:operations ( ) .

:timestampProperty a bamm:Property ;
   bamm:name "timestampProperty" ;
   bamm:characteristic bamm-c:Timestamp .

:numberProperty a bamm:Property ;
   bamm:name "numberProperty" ;
   bamm:characteristic [
     a bamm-c:Quantifiable ;
     bamm:name "SomeLength" ;
     bamm:dataType xsd:unsignedLong ;
     bamm-c:unit unit:metre
   ] .

